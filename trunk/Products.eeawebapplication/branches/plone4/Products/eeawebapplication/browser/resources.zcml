<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           xmlns:five="http://namespaces.zope.org/five">

     <browser:resource name="application.css"
          file="www/application.css" />

     <browser:resource name="shadow.png"
          file="images/shadow-alpha.png" />

     <browser:resource name="application.js"
          file="www/application.js" />

     <browser:resource name="tabs.js"
          file="www/tabs.js" />

     <browser:resource name="ajax.js"
          file="www/ajax.js" />

     <browser:resourceDirectory 
          name="images"
          directory="images" />

</configure>
